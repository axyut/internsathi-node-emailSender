<%- include('partials/header') %>

<div class="form-colm">
	
	<form method="GET" action="/getToken">
			
			<label>Register</label>
			<input
				type="number"
				id="contacts"
				name="Number"
				placeholder="Your Number"
				required
			/>
			<button type="submit">Get Token</button>
		
	
	</form>
</div>

<div class="form-row"></div>
	<form method="POST" action="/searchContact">
		
		
				<button type="submit">Search</button>
				<input
					type="number"
					id="contacts"
					name="number"
					placeholder="Number"
					required
				/>
			
		
	</form>
</div>
<div class="form-row"></div>
	<form method="POST" action="/sendMany/contacts">
		
			
				<button type="submit">Add</button>
				<input
					type="number"
					id="contacts"
					name="contacts"
					placeholder="Contacts"
					required
				/>
			
		
	</form>
</div>

<a href="/all"><button>View All Contacts</button> </a>

<%- include('partials/footer') %>
